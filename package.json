{
  "name": "projeto-bufunfa-serve",
  "version": "1.0.0",
  "description": "O Bufunfa é um aplicativo que ajuda jovens a entender, na prática, como administrar as suas decisões financeiras através de uma empresa virtual fictícia, que demonstra como são as responsabilidades da vida financeira adulta.",
  "main": "index.js",
  "repository": "https://github.com/sergiofisio/projetoBufunfa-serve.git",
  "author": "Equipe Bufunfa",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/multer": "^1.4.8",
    "@types/node": "^20.6.5",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prisma": "^5.5.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "dev": "nodemon src/index.ts",
    "begin": "yarn && yarn prisma && yarn build",
    "start": "node dist/index.js",
    "build": "tsc",
    "prisma": "npm run migrate dev -n bufunfa && npm run generate && npm run push && npm run seed",
    "migrate": "npx prisma migrate dev",
    "generate": "npx prisma generate",
    "pull": "npx prisma db pull",
    "push": "npx prisma db push",
    "seed": "node ./prisma/seed.js"
  },
  "dependencies": {
    "@prisma/client": "^5.5.2",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.3",
    "aws-sdk": "^2.1464.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "fs": "^0.0.1-security",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.5",
    "path": "^0.12.7",
    "swagger-ui-express": "^5.0.0",
    "zod": "^3.22.2"
  }
}
